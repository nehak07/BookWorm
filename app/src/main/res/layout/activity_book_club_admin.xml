<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".BookClubAdminActivity">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="410dp"
        android:layout_height="94dp"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@color/color_button_clicked"
        tools:layout_editor_absoluteX="0dp" />

    <TextView
        android:id="@+id/txt_ClubName_detail"
        android:layout_width="176dp"
        android:layout_height="0dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="128dp"
        android:text="Club Name:"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="25dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/btn_Member_Members"
        android:layout_width="112dp"
        android:layout_height="104dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="140dp"
        android:scaleType="fitCenter"
        android:src="@drawable/members"
        app:layout_constraintStart_toEndOf="@+id/btn_Book_Member"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/btn_Setting_Member"
        android:layout_width="110dp"
        android:layout_height="103dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="28dp"
        android:scaleType="fitCenter"
        android:src="@drawable/setting2"
        app:layout_constraintStart_toEndOf="@+id/btn_Info_Member"
        app:layout_constraintTop_toBottomOf="@+id/btn_Vote_Member" />

    <ImageButton
        android:id="@+id/btn_Book_Member"
        android:layout_width="116dp"
        android:layout_height="99dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="140dp"
        android:scaleType="fitCenter"
        android:src="@drawable/books"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/btn_MeetUp_Members"
        android:layout_width="110dp"
        android:layout_height="103dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="28dp"
        android:scaleType="fitCenter"
        android:src="@drawable/coffee"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_Book_Member" />

    <ImageButton
        android:id="@+id/btn_Vote_Member"
        android:layout_width="110dp"
        android:layout_height="103dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="28dp"
        android:scaleType="fitCenter"
        android:src="@drawable/vote"
        app:layout_constraintStart_toEndOf="@+id/btn_MeetUp_Members"
        app:layout_constraintTop_toBottomOf="@+id/btn_Member_Members" />

    <ImageButton
        android:id="@+id/btn_Info_Member"
        android:layout_width="110dp"
        android:layout_height="103dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="28dp"
        android:scaleType="fitCenter"
        android:src="@drawable/info"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_MeetUp_Members" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="152dp"
        android:layout_marginTop="8dp"
        android:text="ADMIN AREA"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_ClubName_detail" />


</android.support.constraint.ConstraintLayout>