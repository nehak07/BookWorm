<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AdminSettingsActivity">

    <include
        android:id="@+id/AdminAdministration_Toolbar"
        layout="@layout/app_bar_layout"

        ></include>

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="410dp"
        android:layout_height="61dp"
        android:layout_marginTop="48dp"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@color/Button"
        tools:layout_editor_absoluteX="1dp" />

    <TextView
        android:id="@+id/txt_ClubName_detail"
        android:layout_width="176dp"
        android:layout_height="0dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="116dp"
        android:text="Club Name:"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="25dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_ClubAdmin" />

    <TextView
        android:id="@+id/txt_ClubAdmin"
        android:layout_width="358dp"
        android:layout_height="55dp"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="24dp"
        android:text="CLUB ADMINISTRATION"
        android:paddingLeft="10dp"
        android:textAlignment="center"
        android:textColor="@color/White"
        android:textSize="25dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/btn_Setting_Member"
        android:layout_width="118dp"
        android:layout_height="111dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="20dp"
        android:scaleType="fitCenter"
        android:src="@drawable/deletelogo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_Info_Member" />


    <ImageButton
        android:id="@+id/btn_MeetUp_Members"
        android:layout_width="118dp"
        android:layout_height="111dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="20dp"
        android:scaleType="fitCenter"
        android:src="@drawable/meetuplogo"
        app:layout_constraintStart_toEndOf="@+id/btn_Setting_Member"
        app:layout_constraintTop_toBottomOf="@+id/btn_Book_Member" />

    <ImageButton
        android:id="@+id/btn_Info_Member"
        android:layout_width="118dp"
        android:layout_height="111dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="220dp"
        android:scaleType="fitCenter"
        android:src="@drawable/infologo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/btn_Book_Member"
        android:layout_width="118dp"
        android:layout_height="111dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="220dp"
        android:scaleType="fitCenter"
        android:src="@drawable/findbookslogo"
        app:layout_constraintStart_toEndOf="@+id/btn_Info_Member"
        app:layout_constraintTop_toTopOf="parent" />



</android.support.constraint.ConstraintLayout>