<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AdminSettingsActivity">

    <include
        android:id="@+id/AdminAdministration_Toolbar"
        layout="@layout/app_bar_layout"

        ></include>

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="410dp"
        android:layout_height="94dp"
        android:layout_marginTop="48dp"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@color/color_button_clicked"
        tools:layout_editor_absoluteX="1dp" />

    <TextView
        android:id="@+id/txt_ClubName_detail"
        android:layout_width="176dp"
        android:layout_height="0dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="116dp"
        android:text="Club Name:"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="25dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_ClubAdmin" />

    <TextView
        android:id="@+id/txt_ClubAdmin"
        android:layout_width="358dp"
        android:layout_height="55dp"
        android:layout_marginTop="72dp"
        android:layout_marginEnd="24dp"
        android:text="CLUB ADMINISTRATION"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="25dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/btn_Member_Members"
        android:layout_width="112dp"
        android:layout_height="104dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="36dp"
        android:scaleType="fitCenter"
        android:src="@drawable/members"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_Info_Member" />


    <ImageButton
        android:id="@+id/btn_MeetUp_Members"
        android:layout_width="110dp"
        android:layout_height="103dp"
        android:layout_marginStart="32dp"
        android:scaleType="fitCenter"
        android:src="@drawable/coffee"
        app:layout_constraintStart_toEndOf="@+id/btn_Member_Members"
        tools:layout_editor_absoluteY="381dp" />

    <ImageButton
        android:id="@+id/btn_Info_Member"
        android:layout_width="110dp"
        android:layout_height="103dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="100dp"
        android:scaleType="fitCenter"
        android:src="@drawable/info"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <ImageButton
        android:id="@+id/btn_Book_Member"
        android:layout_width="116dp"
        android:layout_height="99dp"
        android:layout_marginStart="224dp"
        android:layout_marginTop="20dp"
        android:scaleType="fitCenter"
        android:src="@drawable/books"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_MeetUp_Members" />

</android.support.constraint.ConstraintLayout>